{
  "name": "@stockalert/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for StockAlert.pro API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "stockalert",
    "stock",
    "market",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "StockAlert.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stockalert/stockalert-js-sdk"
  },
  "bugs": {
    "url": "https://github.com/stockalert/stockalert-js-sdk/issues"
  },
  "homepage": "https://stockalert.pro",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}