{
  "name": "@stockalert/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for StockAlert.pro API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "stockalert",
    "stock",
    "alerts",
    "trading",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "StockAlert.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stockalert-pro/js-sdk"
  },
  "homepage": "https://stockalert.pro",
  "bugs": {
    "url": "https://github.com/stockalert-pro/js-sdk/issues"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
