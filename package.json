{
  "name": "@stockalert/sdk",
  "version": "1.0.1",
  "description": "Official JavaScript/TypeScript SDK for StockAlert.pro API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "stockalert",
    "stock",
    "alerts",
    "trading",
    "api",
    "sdk",
    "typescript",
    "investment",
    "finance"
  ],
  "author": "StockAlert.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stockalert-pro/js-sdk"
  },
  "homepage": "https://stockalert.pro",
  "bugs": {
    "url": "https://github.com/stockalert-pro/js-sdk/issues"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "size-limit": "^11.0.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "15 KB"
    }
  ]
}
